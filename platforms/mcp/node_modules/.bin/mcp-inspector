else
  export NODE_PATH="/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules/@modelcontextprotocol/inspector/cli/build/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules/@modelcontextprotocol/inspector/cli/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules/@modelcontextprotocol/inspector/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules/@modelcontextprotocol/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/node_modules:$NODE_PATH"

  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@modelcontextprotocol+inspector@0.19.0_@cfworker+json-schema@4.1.1_@swc+core@1.15.8_@ty_a22de4885d13c19e553cfbf5b8746438/node_modules/@modelcontextprotocol/inspector/cli/build/cli.js" "$@"

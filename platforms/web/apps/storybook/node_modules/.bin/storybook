#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/bin/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/bin/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/Users/jamiecraik/dev/DesignSystem/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/bin/dispatcher.js" "$@"
else
  exec node  "$basedir/../../../../../../node_modules/.pnpm/storybook@10.1.11_@testing-library+dom@10.4.0_prettier@3.7.4_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/storybook/dist/bin/dispatcher.js" "$@"
fi

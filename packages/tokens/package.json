{
  "name": "@chatui/tokens",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./foundations.css": "./src/foundations.css",
    "./tokens.css": "./src/tokens.css",
    "./tailwind.preset": "./tailwind.preset.ts"
  },
  "files": [
    "src",
    "dist",
    "tailwind.preset.ts"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./src/foundations.css",
    "./src/tokens.css"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist",
    "generate": "tsx scripts/generate-tokens.ts",
    "generate:watch": "tsx watch scripts/generate-tokens.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "fast-check": "^3.15.1",
    "vitest": "^4.0.16"
  }
}
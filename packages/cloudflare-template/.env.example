# Cloudflare Workers Environment Variables
# Copy this file to .env and fill in your values

# Required: The domain where your worker will be deployed
# This should match your Cloudflare Workers domain
WORKER_DOMAIN_BASE="https://my-astudio-widgets.example.workers.dev"

# Optional: Widget domain for OpenAI sandbox
# This is the domain that will be shown to users in ChatGPT
WIDGET_DOMAIN="https://example.com"

# Development settings
NODE_ENV="development"